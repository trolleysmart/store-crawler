{
  "name": "store-crawler",
  "version": "0.1.98",
  "description": "Store Crawler",
  "main": "./dist/index.js",
  "scripts": {
    "start": "babel-node src/index.js",
    "build": "babel src --out-dir dist",
    "lint": "eslint --ext .jsx --ext .js src",
    "fixlint": "eslint --fix --ext .jsx --ext .js src",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test-verbose": "jest --verbose",
    "test-verbose:watch": "npm run test-verbose -- --watch",
    "publish-package": "npm run build && npm publish",
    "flow": "flow",
    "lint:staged": "lint-staged"
  },
  "author": "Micro Business ltd<micro.business.ltd@gmail.com>",
  "homepage": "https://bitbucket.org/microbusiness/store-crawler#readme",
  "dependencies": {
    "crawler": "^1.0.5",
    "immutable": "^3.8.1",
    "micro-business-parse-server-common": "^0.1.68",
    "smart-grocery-parse-server-common": "^0.1.115"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.46.0",
    "jest": "^20.0.3",
    "lint-staged": "^3.4.2",
    "prettier": "^1.3.1",
    "uuid": "^3.0.1"
  },
  "jest": {
    "rootDir": "./src"
  },
  "pre-commit": "lint:staged",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org:microbusiness/store-crawler.git"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote true --trailing-comma all --print-width 150",
      "git add"
    ]
  }
}
