{
  "name": "trolley-smart-store-crawler",
  "version": "1.0.0-alpha.8",
  "description": "Trolley Smart Store Crawler",
  "main": "./dist/index.js",
  "scripts": {
    "start": "babel-node src/index.js",
    "build": "babel src --out-dir dist",
    "lint": "eslint --ext .jsx --ext .js src",
    "fixlint": "eslint --fix --ext .jsx --ext .js src",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test-verbose": "jest --verbose",
    "test-verbose:watch": "npm run test-verbose -- --watch",
    "publish-package": "npm run build && npm publish",
    "flow": "flow",
    "lint:staged": "lint-staged"
  },
  "author": "Micro Business ltd<micro.business.ltd@gmail.com>",
  "homepage": "https://bitbucket.org/microbusiness/trolley-smart-store-crawler#readme",
  "dependencies": {
    "bluebird": "^3.5.0",
    "crawler": "^1.0.5",
    "immutable": "^3.8.1",
    "micro-business-common-javascript": "^1.0.0-alpha.5",
    "micro-business-parse-server-common": "^1.0.2-alpha.21",
    "moment": "^2.18.1",
    "trolley-smart-parse-server-common": "^1.0.0-alpha.5",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.6.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "flow-bin": "^0.54.0",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.4",
    "parse": "^1.10.0",
    "prettier": "^1.6.1"
  },
  "jest": {
    "rootDir": "./src"
  },
  "pre-commit": "lint:staged",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org:microbusiness/trolley-smart-store-crawler.git"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote true --trailing-comma all --print-width 150",
      "git add"
    ]
  }
}
